        }
    },
    "/test/acts/47/5": {
        "1935-04-01": {
            "heading": "Prohibition of beards",
            "content": "The wearing of any beard whatsoever, except as provided in section 6, is prohibited within the Commonwealth of Australia.",
            "children": [],
            "end_date": null,
            "node": "/test/acts/47/5",
            "start_date": "1935-04-01",
